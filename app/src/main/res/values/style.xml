<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="customTextViewFontStyle" parent="@android:style/Widget.DeviceDefault.TextView">
        <item name="android:fontFamily">@font/godo_font</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="customButtonStyle" parent="@android:style/Widget.DeviceDefault.Button">
        <item name="android:fontFamily">@font/godo_font</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:background">@color/white</item>
    </style>
    <style name="customEditTextStyle" parent="@android:style/Widget.DeviceDefault.EditText">
        <item name="android:fontFamily">@font/godo_font</item>
        <item name="android:textColor">@color/black</item>
    </style>
    
    <style name="Splash" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowBackground">@drawable/background_splash</item>
    </style>
</resources>